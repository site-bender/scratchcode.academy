{
	"cleanUrls": true,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Sitebender",
					"value": "scratchcode.academy"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; style-src 'unsafe-inline'; child-src 'none'; img-src https://scratchcode.academy 'self';"
				}
			]
		}
	],
	"redirects": []
}
